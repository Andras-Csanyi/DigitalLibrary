@using DigitalLibrary.MasterData.DomainModel
<BSCard>
    <BSCard CardType="CardType.Title">@DimensionStructure.Name</BSCard>
    @{
        if (_dimensionStructures.Any())
        {
            foreach (DimensionStructure dimensionStructure in _dimensionStructures)
            {
                <DimensionStructureTreeItem DimensionStructure="@dimensionStructure">
                </DimensionStructureTreeItem>
            }
        }
    }
</BSCard>